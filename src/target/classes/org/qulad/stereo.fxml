         <xAxis>
            <NumberAxis side="BOTTOM" tickUnit="1" />
         </xAxis>
         <yAxis>
            <NumberAxis side="LEFT" />
         </yAxis>
      </AreaChart>
      <TextArea fx:id="report" layoutX="489.0" layoutY="18.0" prefHeight="142.0" prefWidth="238.0" />
