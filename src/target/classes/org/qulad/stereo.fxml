   <children>
      <AreaChart fx:id="leftChannelPlot" layoutX="34.0" layoutY="216.0" prefHeight="253.0" prefWidth="1181.0">
        <xAxis>
          <NumberAxis side="BOTTOM" tickUnit="1" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </AreaChart>
      <Button fx:id="applyTransFromBtn" layoutX="517.0" layoutY="187.0" mnemonicParsing="false" onAction="#transformBtnEvent" text="Apply transformation" />
      <Button fx:id="moveToPart2" layoutX="554.0" layoutY="747.0" mnemonicParsing="false" onAction="#movetoPart2Event" text="Move to part 2" />
